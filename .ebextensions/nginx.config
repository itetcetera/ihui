files:
    "/etc/nginx/conf.d/elasticbeanstalk/02_websockets.conf" :
        mode: "000644"
        owner: root
        group: root
        content : |
            location /app/ws {
                proxy_set_header Upgrade $http_upgrade;
                proxy_set_header Connection "upgrade";
                proxy_http_version 1.1;
            }

